{
  "name": "@bluebase/plugin-react-navigation",
  "description": "Use React Navigation in BlueBase apps!",
  "author": "BlueEast Team <code@blueeast.com>",
  "version": "1.7.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.js",
  "repository": "https://github.com/BlueBaseJS/plugin-react-navigation",
  "license": "MIT",
  "keywords": [],
  "config": {},
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "scripts": {
    "start": "yarn run storybook",
    "clean": "rimraf coverage dist tmp",
    "watch": "tsc -w -p tsconfig.json",
    "watch:test": "jest --watch",
    "lint": "eslint src bluebase --ext=ts,tsx",
    "info": "npm-scripts-info",
    "build": "run-s clean && run-p build:* && cp -r ./dist/src/* ./dist && rm -r ./dist/src",
    "build:main": "tsc -p tsconfig.json && babel ./src --out-dir dist --ignore \"**/__tests__\",\"**/__stories__\" --extensions \".ts,.tsx,.js,.jsx\"",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:lint": "eslint src bluebase --ext=ts,tsx --fix .",
    "fix:md": "remark . -o",
    "test": "run-s build test:*",
    "test:only": "jest --coverage --runInBand",
    "test:lint": "npm run lint",
    "test:prettier": "prettier \"src/**/*.ts\" --list-different",
    "test:md": "remark . -f",
    "commit": "git-cz",
    "report-coverage": "codecov && cat ./coverage/lcov.info | codacy-coverage",
    "doc": "run-s doc:html && run-s doc:build-storybook && opn docs/index.html",
    "doc:html": "typedoc src/ --target ES6 --mode file --out docs && cp -r assets docs",
    "doc:json": "typedoc src/ --target ES6 --mode file --json docs/typedoc.json && cp -r assets docs",
    "doc:build-storybook": "build-storybook -c ./bluebase/storybook/configs -o docs/storybook",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d docs",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "all": "run-s reset test cov:check doc:html",
    "prepublishOnly": "yarn remove expo react-native",
    "storybook:start": "bluebase storybook:start",
    "storybook-native": "bluebase storybook-native:start",
    "storybook": "start-storybook -p 6006 --config-dir ./bluebase/storybook/configs",
    "screenshot": "storybook-chrome-screenshot -p 9001 -c ./bluebase/storybook/configs",
    "build-storybook": "build-storybook",
    "web:start": "bluebase web:start",
    "expo:start": "bluebase expo:start"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.8.0",
    "@react-navigation/compat": "^5.2.5",
    "@react-navigation/core": "^5.12.3",
    "@react-navigation/drawer": "^5.9.0",
    "@react-navigation/material-bottom-tabs": "^5.2.16",
    "@react-navigation/material-top-tabs": "^5.2.16",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/native-stack": "^5.0.5",
    "@react-navigation/stack": "^5.9.0",
    "expo": "^38.0.0",
    "lodash.get": "^4.4.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-screens": "~2.9.0",
    "react-native-tab-view": "^2.15.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.11.0",
    "@babel/plugin-transform-flow-strip-types": "^7.8.3",
    "@bluebase/code-standards": "^2.7.0",
    "@bluebase/components": "^6.2.1",
    "@bluebase/core": "^5.11.1",
    "@bluebase/plugin-launcher": "^1.2.0",
    "@bluebase/plugin-react-native-paper": "^3.1.0",
    "@bluebase/plugin-responsive-grid": "^0.0.7",
    "@bluebase/plugin-vector-icons": "^0.3.0",
    "@bluebase/storybook-addon": "^3.0.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/enzyme-async-helpers": "^0.9.1",
    "@types/jest": "^26.0.8",
    "@types/lodash.get": "^4.4.6",
    "@types/react": "~16.9.41",
    "@types/react-dom": "^16.9.5",
    "@types/react-native": "~0.63.45",
    "@types/react-test-renderer": "^16.9.3",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.2.3",
    "codacy-coverage": "^3.4.0",
    "codecov": "^3.6.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-async-helpers": "^0.9.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-order-alphabetical": "^1.0.1",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.2.2",
    "jest-enzyme": "^7.1.2",
    "jest-expo": "^38.0.0",
    "jsdom": "^16.1.0",
    "npm-run-all": "^4.1.3",
    "opn-cli": "^5.0.0",
    "prettier": "^2.0.5",
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "^16.12.0",
    "remark-cli": "^8.0.1",
    "remark-preset-lint-recommended": "^4.0.1",
    "schedule": "^0.5.0",
    "trash-cli": "^3.0.0",
    "ts-jest": "^26.1.4",
    "typedoc": "^0.17.8",
    "typescript": "~3.9.5"
  },
  "peerDependencies": {
    "@bluebase/core": "^6.0.0",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "~3.0.7"
  },
  "prettier": {
    "singleQuote": true,
    "useTabs": true,
    "trailingComma": "es5"
  },
  "publishConfig": {
    "access": "public"
  }
}
